{
  "version": 2,
  "builds": [
    {
      "src": "api/reservas.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/reservas/:id",
      "destination": "/api/reservas"
    },
    {
      "source": "/api/reservas",
      "destination": "/api/reservas"
    },
    {
      "source": "/(.*)",
      "destination": "/build/$1"
    }
  ]
}